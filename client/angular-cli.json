{
  "project": {
    "version": "1.0.0-beta.17",
    "name": "bookapp-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": "assets",
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/ng2-toastr/bundles/ng2-toastr.min.css",
        "fonts.css",
        "styles.css",
        "main.css",
        "annotations.css",
        "popup.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/epubjs/build/epub.js",
        "../node_modules/epubjs/reader/js/hooks.min.js",
        "../node_modules/epubjs/reader/js/reader.min.js",
        "../node_modules/epubjs/reader/js/libs/zip.min.js",
        "../node_modules/epubjs/reader/js/libs/screenfull.min.js",
        "../node_modules/epubjs/reader/js/plugins/hypothesis.js",
        "../node_modules/epubjs/reader/js/plugins/search.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false
  }
}
